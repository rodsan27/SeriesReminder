@model IEnumerable<SERIESREMINDER.Models.CatCorreo>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Agregar", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Correo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Estado)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Correo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Estado)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.IDCorreo }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.IDCorreo }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.IDCorreo })
        </td>
    </tr>
}

</table>
